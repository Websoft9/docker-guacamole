{
  "name": "guacamole",
  "trademark": "Guacamole",
  "installpath": "/data/wwwroot/guacamole",
  "compose_file": "docker-compose.yml",
  "fork_url": "https://github.com/apache/guacamole-server",
  "env_file": ".env",
  "requirements":{
    "cpu_arch": "Linux x86-64, ARM 32/64, Windows x86-64, IBM POWER8, x86/i686",
    "cpu": "2",
    "memory": "4",
    "disk": "20",
    "swap": "2",
    "url": "https://github.com/guacamole/docker#recommended-system-requirements"
  },

  "user": {
    "username": "guacadmin",
    "password": "guacadmin"
  },

  "services":[
    {
      "name": "guacamole-web",
      "port": "9001",
      "description_cn": "浏览器访问 Guacamole",
      "description_en": "Web-GUI database management tool",
      "required": "Y"
    }
  ]

}
